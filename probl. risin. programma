a = input("Введите район, где вы бы хотели работать: ")
b = "Кенгарагс"
c = "Плявниеки"
d = "Пурвциемс"
if a == b:
  print("Доступные вакансии: Мойщик головы - Прушу 7; Кассир винотеки - Локомотивас 46; Флорист - Рушону 12; Учитель - Маскавас 256" )
if a == c:
  print("Доступные вакансии: Снегоуборщик - А. Келдыша 12; Дворник - Илукстес 10; Повар - Тинужу 5; Электрик - Сахарова 15")
if a == d:
  print("Дотупные вакансии: Помошник кассира - Деглава 24; Кассир - Варавикснес 10; Почтальён - Дзеню 3; Няня - Джохара Дудаева 13")
